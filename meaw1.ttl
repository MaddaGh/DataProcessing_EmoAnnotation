<http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw> rdf:type owl:Ontology ;
                                                            rdfs:comment "First modelet of the Multimodal Emotio Annotation Ontology. Developed within my thesis research in collaboration with the netherlands Institute for Sound and Vision." .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw#addresses
meaw:addresses rdf:type owl:ObjectProperty ;
               owl:inverseOf :isAddressedIn .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw#hasSpeaker
meaw:hasSpeaker rdf:type owl:ObjectProperty ;
                owl:inverseOf :isSpeakerIn .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/broadcastedBy
:broadcastedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :TalkShowSituation ;
               rdfs:range :Broadcaster .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/expressedIn
:expressedIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Emotion ;
             rdfs:range :EmotionRelation .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/expressesEmotion
:expressesEmotion rdf:type owl:ObjectProperty ;
                  rdfs:domain :EmotionRelation ;
                  rdfs:range :Emotion .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/hasConversationalContext
:hasConversationalContext rdf:type owl:ObjectProperty ;
                          rdfs:domain :ConversationalRole ;
                          rdfs:range :TalkShowSituation .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/hasFrameContext
:hasFrameContext rdf:type owl:ObjectProperty ;
                 rdfs:domain :EmotionRelation ;
                 rdfs:range :ConversationalFrame .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/hasGender
:hasGender rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range :Gender .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/hasInScope
:hasInScope rdf:type owl:ObjectProperty ;
            rdfs:domain :TalkShowSituation ;
            rdfs:range :EmotionRelation .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:domain :ConversationalRole ;
         rdfs:range :Role .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/hasRoleInConversation
:hasRoleInConversation rdf:type owl:ObjectProperty ;
                       rdfs:domain :Agent ;
                       rdfs:range :ConversationalRole .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/includes
:includes rdf:type owl:ObjectProperty ;
          owl:inverseOf :isIncludedIn ;
          rdfs:domain :BasicEmotion ;
          rdfs:range :EmotionState .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/isAddressedIn
:isAddressedIn rdf:type owl:ObjectProperty ;
               rdfs:domain :Agent ;
               rdfs:range :EmotionRelation .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/isIncludedIn
:isIncludedIn rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/ghiot/ontologies/2023/11/meaw/isSpeakerIn
:isSpeakerIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Agent ;
             rdfs:range :EmotionRelation .


